{
  "name": "@easilybaffled/template-module",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0-development",
  "description": "",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "nyc mocha './src/tests/**/*.test.js' --paralell --growl",
    "test:watch": "npm run test -- --watch",
    "build": "npx pika build",
    "release": "npx np patch --contents=./pkg",
    "package-check": "npx package-check --cwd ./pkg",
    "prettier": "prettier --write  src/",
    "lint": "eslint -f stylish -c .eslintrc.cjs --fix . --ext .js",
    "format": "npm run prettier && npm run lint",
    "semantic-release": "semantic-release"
  },
  "author": "",
  "license": "ISC",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-web",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "@skypack/package-check": "^0.2.2",
    "chai": "^4.3.4",
    "eslint": "^7.23.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "esm": "^3.2.25",
    "firebase-functions-test": "^0.2.3",
    "jest": "^26.6.3",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.2"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/easilyBaffled/firebase-persist.git"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "./pkg"
        }
      ]
    ]
  },
  "dependencies": {
    "firebase": "^8.3.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "p-throttle": "^4.1.1",
    "redux-persist": "^6.0.0"
  },
  "exports": {
    "import": "dist-src/index.js",
    "require": "dist-node/index.js"
  }
}
